{
  "name": "lyrical",
  "version": "1.0.0",
  "description": "Starter point for a graphQL course",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhovagimian/lyrical-graphql"
  },
  "scripts": {
    "dev": "nodemon index.js --ignore client",
    "update-schema": "node ./scripts/updateSchema.js",
    "relay": "npm run update-schema && relay-compiler --src ./ --schema ./server/schema/schema.graphql",
    "relay-watch": "npm run update-schema && relay-compiler --src ./ --schema ./server/schema/schema.graphql --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^4.4.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.2",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-relay": "^11.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^10.1.3",
    "css-loader": "^3.6.0",
    "graphql": "^15.5.1",
    "html-webpack-plugin": "^4.5.2",
    "relay-compiler": "^10.1.3",
    "relay-config": "^10.1.3",
    "style-loader": "^1.3.0",
    "webpack": "^4.46.0",
    "webpack-dev-middleware": "^3.7.3"
  }
}
