{
  "name": "lyrical",
  "version": "1.0.0",
  "description": "Starter point for a graphQL course",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhovagimian/lyrical-graphql"
  },
  "scripts": {
    "dev": "nodemon index.js --ignore client",
    "update-schema": "node ./scripts/updateSchema.js",
    "relay": "npm run update-schema && relay-compiler --src ./ --schema ./server/schema/schema.graphql",
    "relay-watch": "npm run update-schema && relay-compiler --src ./ --schema ./server/schema/schema.graphql --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-session": "^1.17.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.9.25",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^10.0.0",
    "react-router": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-relay": "^10.0.0",
    "css-loader": "^3.6.0",
    "graphql": "^15.3.0",
    "html-webpack-plugin": "^4.3.0",
    "relay-compiler": "^10.0.0",
    "relay-config": "^10.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2"
  }
}
